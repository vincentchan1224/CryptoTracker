﻿@{
    ViewBag.Title = "Create";
}

<h2>Create Price</h2>
<form action="/Prices/Create" method="POST">

    <div>
        <label>Crypto ID:</label> <input type="text" name="Crypto_id" />
    </div>
    <div>
        <label>Price:</label> <input type="text" name="Price" />
    </div>

    <div>
        <label>Timestamp:</label> <input type="date" name="Timestamp" />
    </div>

    <div>
        <input type="submit" value="Add Price" />

    </div>
</form>
